# EditorConfig is awesome: http://EditorConfig.org
#
# To use EditorConfig with one of { Eclipse, Emacs, Sublime, VS, VSCode, Xcode },
# you will need to install a plugin.

root = true

[*]
charset=utf-8-bom
#end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.cs]
charset=utf-8-bom
indent_style = space
end_of_line = crlf

[*.bat]
end_of_line = crlf

[*.md]
trim_trailing_whitespace = false
